      "# Generator of generators"
     ]
    },
    {
     "cell_type": "code",
     "collapsed": false,
     "input": [
      "models = [\n",
      "    (2, 'b', iter([1,2,3,4,5])),\n",
      "    (3, 'r', iter([9,8,7,6,0])),\n",
      "]\n",
      "\n",
      "def m_gen(models):\n",
      "    while True:\n",
      "        yield [next(m[2]) for m in models]\n",
      "    \n",
      "for s in m_gen(models):\n",
      "    print(s)"
     ],
